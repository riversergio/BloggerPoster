<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{title}}</title>
    <!-- Embed -->
    <link rel="stylesheet" href="/css/stylesheets/bootstrap.min.css">
    <link rel="stylesheet" href="/css/stylesheets/all.min.css">
    {{!--
    <link rel="stylesheet" href="/css/stylesheets/animsition.min.css"> --}}
    {{#if styles}}
    {{#each styles}}
    <link rel="stylesheet" href="/css/stylesheets/{{this}}.css">
    {{/each}}
    {{/if}}
    <script src="/js/scripts/jquery-3.6.0.min.js"></script>
    {{!--
    <script src="/js/scripts/animsition.min.js"></script> --}}
    <!-- Main -->
    <link rel="stylesheet" href="/css/index.css">
    <script src="/js/index.js"></script>
</head>

<body>
    <div class="animsition">
        {{> navbar}}
        <main id="main-wrapper">
            <div class="container">
                {{{body}}}
            </div>
        </main>
        {{> footer}}
    </div>
    <script src="/js/scripts/bootstrap.bundle.min.js"></script>
    {{#if scripts}}
    {{#each scripts}}
    <script defer src='/js/{{this}}.js'></script>
    {{/each}}
    {{/if}}
</body>

</html>